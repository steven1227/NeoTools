{
  "name": "neotools",
  "version": "1.0.0",
  "description": "hodgepodge",
  "main": "dist/main.js",
  "scripts": {
    "build": "npx electron-builder -mw",
    "watch": "tsc -w",
    "lint": "eslint -c .eslintrc --ext .ts ./src",
    "start": "tsc && electron ./dist/main.js"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo",
    "typescript"
  ],
  "build": {
    "files": [
      "dist/**/*",
      "html/**/*",
      "neo-sdk/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "build"
    }
  },
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "electron": "^9.4.0",
    "electron-builder": "^22.10.5",
    "electron-compile": "^6.4.4",
    "electron-store": "^8.0.0",
    "eslint": "^7.3.1",
    "rxjs": "^7.0.0",
    "typescript": "^3.9.5"
  }
}
